{
    "json.schemas": [
        {
            "fileMatch": [
                "/.agent/tasks/task_*.json"
            ],
            "url": "./.agent/schemas/plan.schema.json"
        },
        {
            "fileMatch": [
                "/.agent/schemas/*.schema.json"
            ],
            "url": "http://json-schema.org/draft-07/schema"
        }
    ],
    "yaml.schemas": {
        "./.agent/schemas/ontology.schema.json": "/*.yaml"
    },
    "files.associations": {
        "*.orion": "json"
    }
}